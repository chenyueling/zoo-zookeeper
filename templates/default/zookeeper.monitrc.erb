check process zookeeper with pidfile <%= @zk_install_dir %>/zookeeper.pid 
  start program = "/usr/sbin/service zookeeper start"
  stop program = "/usr/sbin/service zookeeper stop"
  if failed port 2181 then restart
  if failed port 2888 then restart
  if failed port 3888 then restart
  if 5 restarts within 5 cycles then timeout
